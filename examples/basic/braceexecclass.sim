block main()
	new BraceUp {
		if Opened {
			display "The classe has been initialized yippie " + nl
		}
	}

class BraceUp

	Opened = false

	block Open()
		if !Opened {
			display "the brace has been opened " + nl
			Opened = true
		}

	block Close()
		if Opened {
			display "the brace has been closed " + nl
			Opened = false
		}
		