from simple.util.Console

colors = 1:1000

for color in colors 
	stdout.Printbc(color, "Color At "+color+" ") 
end
stdout.Println()
for color in colors 
	stdout.Printfc(color, "Color At "+color+" ") 
end
stdout.Println()