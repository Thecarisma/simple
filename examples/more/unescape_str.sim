from simple.core.String

block main()
	str = unEscapeString("Hello\tWorld\\Yes\n")
	display(str)
	str = escapeString(str)
	display(str)
	
	str = unEscapeString("Hello\tWorld\nThis \t is it \n")
	display(str)
	str = escapeString(str)
	display(str)