		if isNumber(index) or instanceOf(index,"Number") {
			if !isNumber(index) {
				_index = index.Number
				index = _index
			}
			if index < 0 || index > Length {
				throw(self,"String index out of bound : " + index)
			}
			return Value[index]
		else
			throw(self,"Invalid parameter, expecting (Number), found (" + objectType(index) + ")")
		}

