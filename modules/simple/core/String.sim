			value = object
		else
			throw(self,"Invalid parameter, expecting (String), found (" + objectType(object) + ")")
		}
		valueLen = lengthOf(value)
		if valueLen != Length {
			return false
		}
		for a = 0 to valueLen {
			if Value[a] != value[a] {
				return false 
			}
		}
		return true

