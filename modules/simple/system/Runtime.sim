from simple.system.loadsystemdll
 
module simple.system

Runtime = new Runtime

class Runtime

	block executeWithParameter(file, param)
		exe = '"'+file+'"' + " "+param
		system(exe)
		
	block execute(exe)
		system('"'+exe+'"')
